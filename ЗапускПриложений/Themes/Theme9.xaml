<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="GradBrush9">
        <GradientStop x:Name="GradientStop17" Offset="0.5" Color="#aaa4e0" />
        <GradientStop x:Name="GradientStop18" Offset="1" Color="#595e98" />
    </LinearGradientBrush>
    <Storyboard x:Key="AnimationTheme9">
        <ColorAnimation
                                AutoReverse="True"
                                RepeatBehavior="Forever"
                                Storyboard.TargetName="{Binding ElementName=GradientStop17}"
                                Storyboard.TargetProperty="Background.Color"
                                From="#aaa4e0"
                                To="#595e98"
                                Duration="0:0:2" />
        <ColorAnimation
                                AutoReverse="True"
                                RepeatBehavior="Forever"
                                Storyboard.TargetName="{Binding ElementName=GradientStop18}"
                                Storyboard.TargetProperty="Background.Color"
                                From="#595e98"
                                To="#aaa4e0"
                                Duration="0:0:2" />
    </Storyboard>
</ResourceDictionary>