<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="GradBrush3">
        <GradientStop x:Name="GradientStop5" Offset="0.5" Color="#3ab3b0" />
        <GradientStop x:Name="GradientStop6" Offset="1" Color="#55367d" />
    </LinearGradientBrush>
    <Storyboard x:Key="AnimationTheme3">
        <ColorAnimation
                                AutoReverse="True"
                                RepeatBehavior="Forever"
                                Storyboard.TargetName="{Binding ElementName=GradientStop5}"
                                Storyboard.TargetProperty="Background.Color"
                                From="#3ab3b0"
                                To="#55367d"
                                Duration="0:0:2" />
        <ColorAnimation
                                AutoReverse="True"
                                RepeatBehavior="Forever"
                                Storyboard.TargetName="{Binding ElementName=GradientStop6}"
                                Storyboard.TargetProperty="Background.Color"
                                From="#3ab3b0"
                                To="#55367d"
                                Duration="0:0:2" />
    </Storyboard>
</ResourceDictionary>