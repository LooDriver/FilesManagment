<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="GradBrush5">
        <GradientStop x:Name="GradientStop9" Offset="0.5" Color="#286987" />
        <GradientStop x:Name="GradientStop10" Offset="1" Color="#002245" />
    </LinearGradientBrush>
    <Storyboard x:Key="AnimationTheme3">
        <ColorAnimation
                                AutoReverse="True"
                                RepeatBehavior="Forever"
                                Storyboard.TargetName="{Binding ElementName=GradientStop9}"
                                Storyboard.TargetProperty="Background.Color"
                                From="#286987"
                                To="#002245"
                                Duration="0:0:2" />
        <ColorAnimation
                                AutoReverse="True"
                                RepeatBehavior="Forever"
                                Storyboard.TargetName="{Binding ElementName=GradientStop10}"
                                Storyboard.TargetProperty="Background.Color"
                                From="#002245"
                                To="#286987"
                                Duration="0:0:2" />
    </Storyboard>
</ResourceDictionary>